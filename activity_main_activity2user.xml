<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity2user">

    <EditText
        android:id="@+id/edtemailusuario"
        android:layout_width="267dp"
        android:layout_height="64dp"
        android:background="@android:drawable/dialog_holo_light_frame"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:text="  Email "
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.452"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.306" />

    <EditText
        android:id="@+id/edtnombreusuario"
        android:layout_width="268dp"
        android:layout_height="58dp"
        android:background="@android:drawable/dialog_holo_light_frame"
        android:ems="10"
        android:inputType="text"
        android:text=" nombre "
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.436"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.437" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="131dp"
        android:layout_height="133dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.464"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.071"
        app:srcCompat="@drawable/usuario" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="351dp"
        android:layout_height="52dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
        app:srcCompat="@drawable/medi_jpg" />

    <ImageButton
        android:id="@+id/imgbtnsetting"
        android:layout_width="135dp"
        android:layout_height="108dp"
        android:rotationX="47"
        app:layout_constraintBottom_toTopOf="@+id/imageView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.173"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtnombreusuario"
        app:layout_constraintVertical_bias="0.505"
        app:srcCompat="@drawable/setting_medical" />

    <ImageButton
        android:id="@+id/imgbtnsalir"
        android:layout_width="127dp"
        android:layout_height="97dp"
        android:scaleType="fitStart"
        app:layout_constraintBottom_toTopOf="@+id/imageView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.554"
        app:layout_constraintStart_toEndOf="@+id/imgbtnsetting"
        app:layout_constraintTop_toBottomOf="@+id/edtnombreusuario"
        app:layout_constraintVertical_bias="0.471"
        app:srcCompat="@drawable/cerrar_sesion_removebg_preview" />

    <LinearLayout
        android:layout_width="355dp"
        android:layout_height="22dp"
        android:background="@android:drawable/dialog_holo_light_frame"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/imgbtnsetting"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtnombreusuario"></LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="384dp"
        android:layout_height="26dp"
        android:background="@android:drawable/dialog_holo_light_frame"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/imageView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.481"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgbtnsetting"
        app:layout_constraintVertical_bias="0.571"></LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>